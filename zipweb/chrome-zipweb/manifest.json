{
  "manifest_version": 2,

  "name": "ZipWeb",
  "description": "Zip web resources to read deep offline",
  "version": "1.0",
  "background":{
	"scripts":["background.js","zip.js","deflate.js"]
  },
  "icons": {
		"16": "img/icon.png",
		"48": "img/icon.png",
		"256": "img/icon.png"
   },
   "permissions":["tabs","contextMenus","<all_urls>"],
   "content_scripts":[
		{
			"matches":["<all_urls>"],
			"js":["content.js"],
			"run_at": "document_start"
		}
		]
}