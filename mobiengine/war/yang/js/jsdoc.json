{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
		"include":["main.js", "deps/app.js", "model.js", 
			"view/base.js", "view/user.js", "view/test.js", "view/splash.js"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
